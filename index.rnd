    <script>
    // Initilise the gallery which has been copied into the overlay.
    document.body.addEventListener('oOverlay.ready', () => {
        const overlayGallery = document.querySelector('.o-overlay [data-o-gallery--js]');
        if (overlayGallery && !overlayGallery.hasAttribute('data-overlay-initiated')) {
            overlayGallery.setAttribute('data-overlay-initiated', "");
            overlayGallery.removeAttribute('data-o-gallery--js');
            Origami["o-gallery"].init();
        }
    });

    // Recalculate overlay position when gallery is ready by opening/closing it.
    document.body.addEventListener('oGallery.ready', () => {
        const overlays = Origami["o-overlay"].getOverlays();
        for (const [key, overlay] of Object.entries(overlays)) {
            overlay.close();
            overlay.open();
        }
    });
    </script>
    
